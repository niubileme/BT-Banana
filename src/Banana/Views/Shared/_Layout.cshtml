    <div class="clearfix"></div>
    <div class="container clearfix">
        @RenderBody()
    </div>
    @Html.Partial("_Footer")
    <script src="https://cdn.bootcss.com/jQuery-Paging/1.2.0/jquery.paging.min.js"></script>
    <script src="~/js/bootsnav.min.js" asp-append-version="true"></script>
    <script>
        $(function () {
            $("#msearch").click(function () {
                var key = $.trim($("#mkey").val());
                if (key == "" || key.length <= 0) {
                    //console.log("请先输入参数")
                } else {
                    window.location.href = '/s/magnet/' + encodeURIComponent(key);
                }
            });
        })
    </script>
    @RenderSection("FootScripts", false)
