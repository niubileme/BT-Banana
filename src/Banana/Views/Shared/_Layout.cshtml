    <script src="https://cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    @RenderSection("HeadScripts", false)
</head>
<body>
    <header class="gnome-header">
        <nav class="navbar navbar-default clearfix">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-wrapper" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar top-bar"></span>
                        <span class="icon-bar middle-bar"></span>
                        <span class="icon-bar bottom-bar"></span>
                    </button>
                    <a id="home-link" class="gnome-navbar-brand" title="BT香蕉" href="/">BT香蕉</a>
                </div>
                <div class="navbar-collapse collapse" id="navbar-wrapper" aria-expanded="false" style="height: 1px;">
                    <ul class="nav navbar-nav">
                        <li><a title="首页" href="/">首页</a></li>
                        <li><a title="电影库" href="/video">电影库</a></li>
                    </ul>
                    <form id="searchform" method="get" class="navbar-form navbar-right hidden-xs" action="/">
                        <label for="navbar-search" class="sr-only">Search:</label>
                        <div class="form-group has-feedback has-feedback-left feedback-container">
                            <span class="form-control-feedback"><i class="fa fa-search" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="s" id="navbar-search" value="" placeholder="Search" autocomplete="off">
                        </div>
                    </form>
                </div>
            </div>
        </nav>
    </header>
    <!-- Start Navigation -->
