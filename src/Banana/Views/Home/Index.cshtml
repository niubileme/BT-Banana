
@{
    Layout = null;
    var BaiDuHot = ViewData["BaiDuHot"] as List<string>;
}
<!DOCTYPE HTML>
<html>
<head>
    <meta charset="UTF-8">
    <title>BT香蕉 - 最好用的磁力链接搜索引擎(btbanana.com)</title>
    <meta name="keywords" content="磁力搜索,磁力链接,种子搜索,BT搜索,视频解析,电影库">
    <meta name="description" content="BT香蕉(btbanana.com)是一个拥有资源最多，更新速度最快的磁力链接搜索引擎，提供最新最热的bt资源、高效精准的垂直搜索服务。">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta http-equiv="cache-control" content="no-siteapp">
    <link href="https://cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" />
    <link href="https://cdn.bootcss.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="https://cdn.bootcss.com/animate.css/3.5.2/animate.min.css" rel="stylesheet">
    <link href="~/css/bootsnav.min.css" rel="stylesheet" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Start Navigation -->
    <nav class="navbar navbar-default navbar-mobile navbar-fixed navbar-transparent bootsnav">

        <div class="container">
            <!-- Start Header Navigation -->
            <div class="navbar-header visible-xs-block visible-sm-block">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-menu">
                    <i class="fa fa-bars"></i>
                </button>
                <a class="navbar-brand" href="#brand">
                    BT香蕉
                </a>
            </div>
            <!-- End Header Navigation -->
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="navbar-menu">
                <ul class="nav navbar-nav navbar-left">
                    <li><a href="/">首页</a></li>
                    @*<li><a href="/s">视频搜索</a></li>
                        <li><a href="/analyse" target="_blank">解析接口</a></li>*@
                    <li><a href="/video" target="_blank">电影库</a></li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
    </nav>
    <!-- End Navigation -->
    <div class="clearfix"></div>
    <div class="container pagehead">

        <div class="row">
            <div class="col-md-12">
                <div class="col-md-6 col-md-offset-3">
                    <a href="/" style="display: block; margin: 70px 0;">
                        <img class="img-responsive center-block" src="~/images/logo_b.png" alt="btbanana.com search logo" title="btbanana.com" />
                    </a>
                </div>
                <form class="form col-md-6 col-md-offset-3" onsubmit="return false;">
                    <div class="input-group">
                        <input type="text" class="form-control" name="key" id="ikey">
                        <span class="input-group-btn">
                            <input class="btn btn-success" type="submit" value="搜索" id="searchForm">
                        </span>
                    </div>
                </form>
            </div>
        </div>

        <div class="row">
            <div class="col-md-10 col-md-offset-1" style="margin-top:30px; margin-bottom: 40px;">
                <div class="hotkey">
                    @{
                        if (BaiDuHot != null)
                        {
                            foreach (var item in BaiDuHot)
                            {
                                <a href="/s/magnet/@item" target="_blank">@item</a>
                            }
                        }
                    }
                </div>
            </div>
        </div>

    </div>
    @Html.Partial("_Footer")
    <script src="https://cdn.bootcss.com/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="~/js/bootsnav.min.js" asp-append-version="true"></script>
    <script>
        $(function () {

            function submitSearch() {
                var key = $.trim($("#ikey").val());
                if (key === "" || key.length <= 0) {
                    //alert("请先输入参数")
                } else {
                    window.open('/s/magnet/' + encodeURIComponent(key));
                }
            }
            $('#ikey').bind('keypress', function (event) {
                if (event.keyCode === "13") {
                    submitSearch();
                }
            });

            $('#searchForm').bind('click', function (event) {
                submitSearch();
            });

            $("#ikey").focus().select();

        });
    </script>
</body>
</html>