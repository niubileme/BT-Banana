
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>1111</title>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta name="keywords" content=" " />
    <meta name="description" content=" " />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdn.bootcss.com/todc-bootstrap/3.3.7-3.3.13/css/bootstrap-theme.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
</head>
<body>

    <div class="container-fluid">
        <div class="row-fluid">
            <div class="col-md-12 channel-menu">
                <div class="pull-right float-middle">
                    <select id="lang" onchange="this.options[this.selectedIndex].value && (window.location = this.options[this.selectedIndex].value);">
                        <option value="/setlang?lang=zh-CN" @selected("zh-cn", culture)>
                            简体中文
                        </option>
                        <option value="/setlang?lang=en-US" @selected("en-us", culture)>
                            English
                        </option>
                    </select>
                </div>
            </div>
        </div>
        <div class="row-fluid" style="margin-top:150px">
            <form action="/s" method="get" class="col-md-6 col-md-offset-3 center" id="searchForm">
                <div class="main-search-logo">
                    <a href="/">
                        <img src="~/Content/img/logo_b.png" alt="btbanana.com search logo" title="btbanana.com">
                    </a>

                    <h1>tips</h1>
                </div>
                <div class="main-search-input">


                    <input type="text" placeholder="IndexInputPlaceholder" name="q" class="form-control input-lg input-search" />
                </div>
                <div class="main-search-btn">
                    <input type="submit" value="搜索" class="btn btn-default btn-lg btn-main-search">
                </div>
            </form>
        </div>

        <div class="row-fluid">
            <div class="col-md-6 col-md-offset-3" style="margin-top:30px; margin-bottom: 40px;">
                <div class="hotkey">

                </div>
            </div>
        </div>

    </div>
    <script src="https://cdn.bootcss.com/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="https://cdn.bootcss.com/clipboard.js/1.7.1/clipboard.min.js"></script>
    <script>
        $(function () {
            $.ajax({ type: 'POST', url: '/GetBaiDuHotKeys', data: { n: 10 }, success: function (data) { if (data.msg) { $.each(data.data, function (i, v) { $('<a href="/s/' + v + '">' + v + '</a>&nbsp;&nbsp;&nbsp;&nbsp').appendTo('.hotkey'); }); } } });
        })
    </script>
</body>
</html>