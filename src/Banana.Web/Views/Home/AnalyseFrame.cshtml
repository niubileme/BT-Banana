
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AnalyseFrame</title>
</head>
<body>
    <input type="hidden" id="url" value="@ViewData["url"]" />
    <input type="hidden" id="gkey" value="@ViewData["gkey"]" />
    <input type="hidden" id="timestamp" value="@ViewData["timestamp"]" />
    <input type="hidden" id="sign" value="@ViewData["sign"]" />
    <div id="video" style="width: 100%;height: 100%; position: absolute;top: 0;bottom: 0;left:0;right:0;"></div>
    <script src="~/ckplayer/ckplayer.js" charset="utf-8"></script>
    <script>
        window.onload = function () {
            var b = {
                f: '/analyse/core?url=' + document.getElementById("url").value + '&gkey=' + document.getElementById("gkey").value + '&timestamp=' + document.getElementById("timestamp").value + '&sign=' + document.getElementById("sign").value,
                c: 0,
                s: 1,
                p: 1,
                b: 1,
            };
            var c = {
                bgcolor: '#FFF',
                allowFullScreen: true,
                allowScriptAccess: 'always',
                wmode: 'transparent'
            };
            CKobject.embedSWF('/ckplayer/ckplayer.swf', 'video', 'ck', '100%', '100%', b, c)
        };
    </script>
</body>
</html>
